{
    "name": "jd/jd-headless-api",
    "type": "typo3-cms-extension",
    "description": "",
    "license": ["GPL-2.0-or-later"],
    "keywords": ["TYPO3 CMS"],
    "require": {
        "typo3/cms-core": "^13.4",
        "typo3/cms-rte-ckeditor": "^13.4",
        "typo3/cms-fluid-styled-content": "^13.4"
    },
    "autoload": {
        "psr-4": {
            "JD\\JdHeadlessApi\\": "Classes/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "JD\\JdHeadlessApi\\Tests\\": "Tests/"
        }
    },
    "extra": {
        "typo3/cms": {
            "extension-key": "jd_headless_api"
        }
    },
    "require-dev": {
        "typo3/testing-framework": "^9.2"
    },
    "config": {
        "vendor-dir": ".Build/vendor",
        "bin-dir": ".Build/bin",
        "allow-plugins": {
            "typo3/class-alias-loader": true,
            "typo3/cms-composer-installers": true
        }
    }
}
